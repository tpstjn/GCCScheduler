<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" stylesheets="@styles/base.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.scheduler.gccscheduler.EditView">
    <children>
        <Label layoutX="674.0" layoutY="5.0" text="From the Flamingoes" textFill="#e747ff" />
        <Label fx:id="pageLabel" layoutX="14.0" layoutY="5.0" />
        <MenuButton fx:id="btnUserMenu" layoutX="672.0" layoutY="26.0" mnemonicParsing="false" text="My Account">
            <items>
                <MenuItem mnemonicParsing="false" onAction="#onLogoutButtonClick" text="Log Out" />
                <MenuItem mnemonicParsing="false" onAction="#onEditButtonClick" text="Edit Account" />
            </items>
        </MenuButton>
      <TextField layoutX="335.0" layoutY="118.0" promptText="Graduation Year" />
      <TextField layoutX="335.0" layoutY="156.0" promptText="Major" />
      <Button layoutX="339.0" layoutY="193.0" mnemonicParsing="false" onAction="#onCancelButtonClick" text="Cancel" />
      <Button layoutX="425.0" layoutY="193.0" mnemonicParsing="false" onAction="#onSubmitButtonClick" text="Submit" />
      <Button layoutX="14.0" layoutY="26.0" mnemonicParsing="false" onAction="#back" text="Back" />

    </children>

</AnchorPane>
